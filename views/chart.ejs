<!DOCTYPE html>

<html>

<!-- Head -->
<head>

    <!-- Page title-->
    <title><%= chartPageTitle %></title>

    <!-- Meta tags -->
    <meta charset="UTF-8">

    <!-- CSS stylesheet -->
    <link rel="stylesheet" href="/static/css/chart-web.css">
    <link rel="stylesheet" href="/static/css/chart/<%= chartStylesheet %>">

    <!-- External scripts (JQuery, D3.js) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.0/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/d3js/7.8.4/d3.min.js"></script>

    <!-- Custom scripts -->
    <script src="/static/js/chart-web.js"></script>
    <script src="/static/js/chart/<%= chartScript %>"></script>

</head>

<!-- Body -->
<body>

<div id="content-wrapper">

    <!-- Wrapper for all elements of the chart - header, body, and footer -->
    <div id="chart-wrapper">

        <!-- Row at the top of the chart, displays chart title -->
        <div id="chart-header">

            <div>
                <h1 id="chart-title"><%= chartTitle %></h1>
                <h2 id="chart-subtitle"><%= chartSubtitle %></h2>
            </div>

        </div>

        <!-- Chart body, generated with d3 in the browser -->
        <div id="chart-body"></div>

        <!-- Optional text description to accompany the chart -->
        <div id="chart-description"></div>

    </div>

</div>

</body>

</html>
