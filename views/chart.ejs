<!DOCTYPE html>

<html>

<!-- Head -->
<head>

    <!-- Page title-->
    <title><%= chartPageTitle %></title>

    <!-- Meta tags -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- CSS stylesheet -->
    <link rel="stylesheet" href="/static/css/chart-web.css">
    <link rel="stylesheet" href="/static/css/chart/<%= chartStylesheet %>">

    <!-- External scripts (JQuery, D3.js) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.0/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/d3js/7.8.4/d3.min.js"></script>

    <!-- Custom scripts -->
    <script src="/static/js/chart-web.js"></script>
    <script src="/static/js/chart/<%= chartScript %>"></script>

    <!-- Load Google fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="">
    <link href="https://fonts.googleapis.com/css2?family=Crimson+Text:ital,wght@0,400;0,700;1,400;1,700&amp;display=swap" rel="stylesheet">

</head>

<!-- Body -->
<body>

<div id="content-wrapper">

    <!-- Wrapper for all elements of the chart - header, body, and footer -->
    <div id="chart-wrapper">

        <!-- Row at the top of the chart, displays chart title -->
        <div id="chart-header">

            <div>
                <h1 id="chart-title"><%= chartTitle %></h1>
                <h2 id="chart-subtitle"><%= chartSubtitle %></h2>
            </div>

            <h2 id="chart-logo-text">Jake<br>Gloudemans</h2>


        </div>

        <!-- Chart body, generated with d3 in the browser -->
        <div id="chart-body">

            <!-- Tooltip box, overlayed on chart SVG -->
            <div id="tooltip-box">
                <h3 id="tooltip-date">August 30, 2023</h3>

                <div class="tooltip-row">
                    <div class="tooltip-icon" id="tooltip-icon-hist"></div>
                    <p class="tooltip-text"><span class="tooltip-text-label">Hist. Avg:</span> <span id="tooltip-text-val-hist">26.7</span></p>
                </div>

                <div class="tooltip-row">
                    <div class="tooltip-icon" id="tooltip-icon-csu"></div>
                    <p class="tooltip-text"><span class="tooltip-text-label">CSU Model:</span> <span id="tooltip-text-val-csu">42.4</span></p>
                </div>

                <div class="tooltip-row">
                    <div class="tooltip-icon" id="tooltip-icon-actual"></div>
                    <p class="tooltip-text"><span class="tooltip-text-label">Actual:</span> <span id="tooltip-text-val-actual">29.6</span></p>
                </div>

                <div class="tooltip-row">
                    <div class="tooltip-icon" id="tooltip-icon-proj"></div>
                    <p class="tooltip-text"><span class="tooltip-text-label">9/31 Proj:</span> <span id="tooltip-text-val-proj">121.4</span></p>
                </div>

                <div class="tooltip-row">
                    <div class="tooltip-icon" id="tooltip-icon-iqr"></div>
                    <p class="tooltip-text"><span class="tooltip-text-label">9/31 IQR:</span> <span id="tooltip-text-val-iqr">99.2 - 134.5</span></p>
                </div>

            </div>

        </div>

        <!-- Optional text description to accompany the chart -->
        <div id="chart-description"><%- chartDescription %></div>

    </div>

</div>

</body>

</html>
